{
  "name": "@socketregistry/packageurl-js",
  "version": "1.0.9",
  "license": "MIT",
  "description": "Socket.dev optimized package override for packageurl-js",
  "keywords": [
    "Socket.dev",
    "package-overrides"
  ],
  "homepage": "https://github.com/SocketDev/socket-packageurl-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SocketDev/socket-packageurl-js.git"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./data/npm/builtin-names.json": "./data/npm/builtin-names.json",
    "./data/npm/legacy-names.json": "./data/npm/legacy-names.json",
    "./src/constants": "./src/constants.js",
    "./src/decode": "./src/decode.js",
    "./src/encode": "./src/encode.js",
    "./src/error": "./src/error.js",
    "./src/helpers": "./src/helpers.js",
    "./src/lang": "./src/lang.js",
    "./src/normalize": "./src/normalize.js",
    "./src/objects": "./src/objects.js",
    "./src/package-url": {
      "types": "./src/package-url.d.ts",
      "default": "./src/package-url.js"
    },
    "./src/purl-component": "./src/purl-component.js",
    "./src/purl-qualifier-names": "./src/purl-qualifier-names.js",
    "./src/purl-type": "./src/purl-type.js",
    "./src/strings": "./src/strings.js",
    "./src/validate": "./src/validate.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "check": "run-p -c --aggregate-output check:*",
    "check-ci": "pnpm check:lint",
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:lint:fix": "pnpm check:lint --fix",
    "check:tsc": "tsc",
    "clean": "run-s -c clean:*",
    "clean:cache": "del-cli '**/.cache'",
    "clean:coverage": "del-cli 'coverage'",
    "clean:node_modules": "del-cli '**/node_modules'",
    "coverage": "run-s coverage:*",
    "coverage:test": "pnpm test:unit:coverage",
    "coverage:percent": "node scripts/get-coverage-percentage.mjs",
    "coverage:type": "dotenvx -q run -f .env.local -- type-coverage --detail",
    "fix": "run-s lint:fix check:lint:fix",
    "knip:dependencies": "knip --dependencies",
    "knip:exports": "knip --include exports,duplicates",
    "lint": "dotenvx -q run -f .env.local -- oxlint -c=.oxlintrc.json --ignore-path=.oxlintignore --tsconfig=tsconfig.json .",
    "lint:fix": "run-s -c lint:fix:*",
    "lint:fix:oxlint": "dotenvx -q run -f .env.local -- oxlint -c=.oxlintrc.json --ignore-path=.oxlintignore --tsconfig=tsconfig.json --quiet --fix . | dev-null",
    "lint:fix:biome": "dotenvx -q run -f .env.local -- biome format --log-level=none --fix . | dev-null",
    "lint:fix:eslint": "dotenvx -q run -f .env.local -- eslint --report-unused-disable-directives --fix . | dev-null",
    "lint-staged": "dotenvx -q run -f .env.local -- lint-staged",
    "precommit": "lint-staged",
    "prepare": "husky",
    "test": "run-s check test:*",
    "test:unit": "dotenvx -q run -f .env.test -- vitest --run",
    "test:unit:coverage": "dotenvx -q run -f .env.test -- vitest run --coverage",
    "test:unit:update": "dotenvx -q run -f .env.test -- vitest --run --update",
    "test-ci": "pnpm test",
    "test-pre-commit": "dotenvx -q run -f .env.precommit -- pnpm test",
    "update": "run-p --aggregate-output update:* && run-s update:longtask:**",
    "update:deps": "taze",
    "update:socket": "pnpm update '@socketsecurity/*' --latest",
    "update:longtask:data:npm": "node ./scripts/update-data-npm.js"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@dotenvx/dotenvx": "1.49.0",
    "@eslint/compat": "1.3.2",
    "@eslint/js": "9.35.0",
    "@socketsecurity/registry": "1.1.17",
    "@vitest/coverage-v8": "3.2.4",
    "all-the-package-names": "2.0.0",
    "all-the-package-names-v1.3905.0": "npm:all-the-package-names@1.3905.0",
    "del-cli": "6.0.0",
    "dev-null-cli": "2.0.0",
    "eslint": "9.35.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-unicorn": "56.0.1",
    "fast-glob": "3.3.3",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "knip": "5.63.1",
    "lint-staged": "16.1.6",
    "npm-run-all2": "8.0.4",
    "oxlint": "1.15.0",
    "pacote": "21.0.1",
    "semver": "7.7.2",
    "taze": "19.6.0",
    "type-coverage": "2.29.7",
    "typescript": "5.9.2",
    "validate-npm-package-name": "6.0.2",
    "vitest": "3.2.4",
    "yargs-parser": "21.1.1",
    "yoctocolors-cjs": "2.1.3"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=10.16.0"
  },
  "files": [
    "*{.js,.ts}",
    "data/**/*.json",
    "src/**/*{.js,.ts}",
    "CHANGELOG.md"
  ],
  "lint-staged": {
    "*.{cjs,js,json,md,mjs,mts,ts}": [
      "pnpm lint:fix:oxlint",
      "pnpm lint:fix:biome -- --no-errors-on-unmatched --files-ignore-unknown=true --colors=off"
    ]
  },
  "pnpm": {
    "overrides": {
      "vite": "7.1.5"
    }
  },
  "socket": {
    "categories": [
      "levelup"
    ]
  }
}
