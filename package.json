{
  "name": "@socketregistry/packageurl-js",
  "version": "1.1.4",
  "license": "MIT",
  "description": "Socket.dev optimized package override for packageurl-js",
  "keywords": [
    "Socket.dev",
    "package-overrides"
  ],
  "homepage": "https://github.com/SocketDev/socket-packageurl-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SocketDev/socket-packageurl-js.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./data/npm/builtin-names.json": "./data/npm/builtin-names.json",
    "./data/npm/legacy-names.json": "./data/npm/legacy-names.json",
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "check": "run-p -c --aggregate-output check:*",
    "lint-ci": "pnpm run check:lint",
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:lint:fix": "pnpm run check:lint --fix",
    "build": "pnpm run clean:dist && tsgo --build",
    "check:tsc": "tsgo --noEmit -p tsconfig.check.json",
    "clean:dist": "del-cli 'dist' '**/*.tsbuildinfo'",
    "clean": "run-s -c clean:*",
    "clean:cache": "del-cli '**/.cache'",
    "clean:coverage": "del-cli 'coverage'",
    "clean:node_modules": "del-cli '**/node_modules'",
    "coverage": "run-s coverage:*",
    "coverage:test": "run-s pretest:unit test:unit:coverage",
    "coverage:percent": "node scripts/get-coverage-percentage.mjs",
    "coverage:type": "type-coverage --at-least 96",
    "coverage:type:verbose": "type-coverage --detail",
    "fix": "run-s lint:fix check:lint:fix",
    "knip:dependencies": "knip --dependencies",
    "knip:exports": "knip --include exports,duplicates",
    "lint": "oxlint -c=.oxlintrc.json --ignore-path=.oxlintignore --tsconfig=tsconfig.json .",
    "lint:fix": "run-s -c lint:fix:*",
    "lint:fix:oxlint": "oxlint -c=.oxlintrc.json --ignore-path=.oxlintignore --tsconfig=tsconfig.json --quiet --fix . | dev-null",
    "lint:fix:biome": "biome format --log-level=none --fix . | dev-null",
    "lint:fix:eslint": "eslint --report-unused-disable-directives --fix . | dev-null",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "prepare": "husky",
    "prepublishOnly": "echo 'ERROR: Use GitHub Actions workflow for publishing' && exit 1",
    "pretest:unit": "dotenvx -q run -f .env.test -- pnpm run build",
    "test": "run-s check test:*",
    "test:unit": "dotenvx -q run -f .env.test -- vitest --run",
    "test:unit:coverage": "dotenvx -q run -f .env.test -- vitest run --coverage",
    "test:unit:update": "dotenvx -q run -f .env.test -- vitest --run --update",
    "test-ci": "dotenvx -q run -f .env.test -- vitest --run",
    "type-ci": "pnpm run check:tsc",
    "test-pre-commit": "dotenvx -q run -f .env.precommit -- pnpm test",
    "update": "run-p --aggregate-output update:* && run-s update:longtask:**",
    "update:deps": "node scripts/taze.mjs",
    "update:socket": "pnpm -r update '@socketsecurity/*' '@socketregistry/*' --latest",
    "update:longtask:data:npm": "node ./scripts/update-data-npm.mjs"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@dotenvx/dotenvx": "1.49.0",
    "@eslint/compat": "1.3.2",
    "@eslint/js": "9.35.0",
    "@socketsecurity/registry": "1.3.9",
    "@types/node": "24.5.2",
    "@typescript/native-preview": "7.0.0-dev.20250926.1",
    "@vitest/coverage-v8": "3.2.4",
    "all-the-package-names": "2.0.0",
    "all-the-package-names-v1.3905.0": "npm:all-the-package-names@1.3905.0",
    "del-cli": "6.0.0",
    "dev-null-cli": "2.0.0",
    "eslint": "9.35.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-unicorn": "56.0.1",
    "fast-glob": "3.3.3",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "knip": "5.63.1",
    "lint-staged": "16.1.6",
    "npm-run-all2": "8.0.4",
    "oxlint": "1.15.0",
    "pacote": "21.0.1",
    "semver": "7.7.2",
    "taze": "19.6.0",
    "type-coverage": "2.29.7",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "validate-npm-package-name": "6.0.2",
    "vitest": "3.2.4",
    "yargs-parser": "22.0.0",
    "yoctocolors-cjs": "2.1.3"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/**/*",
    "data/**/*.json",
    "CHANGELOG.md"
  ],
  "lint-staged": {
    "*.{cjs,js,json,md,mjs,mts,ts}": [
      "pnpm run lint:fix:oxlint",
      "pnpm run lint:fix:biome -- --no-errors-on-unmatched --files-ignore-unknown=true --colors=off"
    ]
  },
  "pnpm": {
    "overrides": {
      "vite": "7.1.5"
    }
  },
  "socket": {
    "categories": [
      "levelup"
    ]
  }
}
