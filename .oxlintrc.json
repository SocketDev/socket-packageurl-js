{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["import", "promise", "typescript", "unicorn"],
  "categories": {
    "correctness": "warn",
    "perf": "warn",
    "suspicious": "warn"
  },
  "settings": {},
  "rules": {
    "no-control-regex": "off",
    "no-extend-native": "off",
    "no-new": "off",
    "no-self-assign": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-var": "error",
    "unicorn/no-empty-file": "off",
    "unicorn/no-new-array": "off"
  }
}
